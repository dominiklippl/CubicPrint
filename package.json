{
  "name": "cubicprint",
  "version": "1.0.0",
  "description": "Simple 3D printer server, mainly for the anycubic i3 mega S",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "author": {
    "name": "Dominik Lippl",
    "email": "mail@dominiklippl.de",
    "url": "https://dominiklippl.de"
  },
  "repository": {
    "url": "https://github.com/dominiklippl/cubicprint.git"
  },
  "scripts": {
    "build": "turbo run build",
    "build:backend": "cd apps/backend && yarn build",
    "dev:backend": "cd apps/backend && yarn serve",
    "format": "cd apps/backend && yarn format"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "dependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.5",
    "@semantic-release/npm": "^9.0.1",
    "semantic-release": "^19.0.3"
  }
}
